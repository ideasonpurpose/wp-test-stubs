{
  "name": "wp-test-stubs",
  "version": "0.2.0",
  "description": "Simple collection of stubs and doubles for testing WordPress code",
  "keywords": [
    "phpunit",
    "wordpress",
    "Ideas On Purpose"
  ],
  "homepage": "https://github.com/ideasonpurpose/wp-test-stubs#readme",
  "bugs": {
    "url": "https://github.com/ideasonpurpose/wp-test-stubs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideasonpurpose/wp-test-stubs.git"
  },
  "license": "MIT",
  "author": "Ideas On Purpose (https://www.ideasonpurpose.com)",
  "contributors": [
    "Joe Maller <joe@ideasonpurpose.com>"
  ],
  "scripts": {
    "composer": "npm run composer:install",
    "composer:install": "docker-compose run --rm composer",
    "composer:require": "docker-compose run --rm composer require",
    "composer:update": "docker-compose run --rm composer update",
    "version": "version-everything && auto-changelog && git add -u"
  },
  "prettier": "@ideasonpurpose/prettier-config",
  "devDependencies": {
    "@ideasonpurpose/prettier-config": "^0.0.4",
    "@prettier/plugin-php": "^0.22.2",
    "auto-changelog": "^2.4.0",
    "prettier": "^3.2.5",
    "version-everything": "^0.11.0"
  },
  "auto-changelog": {
    "package": true,
    "backfillLimit": 7
  },
  "version-everything": {
    "files": [
      "README.md"
    ]
  }
}
